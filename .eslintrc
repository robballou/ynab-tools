{
  "extends": "airbnb-base/legacy",
  "env": {
    "es6": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": ["error", "always-multiline"],
    "lines-between-class-members": "off",
  }
}
